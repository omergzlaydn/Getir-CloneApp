    "@babel/core": "^7.12.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/jest-native": "^4.0.5",
    "@testing-library/react-native": "^10.1.1",
    "@types/jest": "^28.1.4",
    "@types/react": "~17.0.21",
